    "import torch\n",
    "\n",
    "# 形状が(3, 1, 2)のテンソルを作成\n",
    "x = torch.randn(3, 1, 2)\n",
    "x_squeezed = x.squeeze()\n",
    "x_unsqueezed = x.unsqueeze(0)\n",
    "\n",
    "print(x)\n",
    "print(x_squeezed)\n",
    "print(x_unsqueezed)"
